{
    "common": {
        "name": "komoot",
        "version": "0.3.1",
        "news": {
            "0.2.0": {
                "en": "Receive recorded tours and statistic data",
                "de": "Aufgezeichnete Touren und statistische Daten abrufen"
            },
            "0.1.0": {
                "en": "Receive UserId and lastTourId. Authorize on Komoot.",
                "de": "userId und letzte TourId abrufen. Authorisieren bei Komoot."
            }
        },
        "title": "KOMOOT",
        "titleLang": {
            "en": "KOMOOT",
            "de": "KOMOOT"
        },
        "desc": {
            "en": "Fetch Komoot Data",
            "de": "Komoot Daten abrufen"
        },
        "authors": [
            "basti4557 <sebastian@vindicators.de>"
        ],
        "license": "MIT",
        "platform": "Javascript/Node.js",
        "mode": "daemon",
        "compact": true,
        "availableModes": [
            "daemon"
        ],
        "schedule": "*/1 * * * *",
        "readme": "https://github.com/basti4557/ioBroker.komoot/blob/main/README.md",
        "loglevel": "info",
        "icon": "komoot.png",
        "keywords": [
            "komoot",
            "bike",
            "ride"
        ],
        "type": "network",
        "plugins": {
        },
        "materialize": true,
        "dependencies": [
            {
                "js-controller": ">=2.0.0"
            }
        ],
        "connectionType": "local",
        "dataSource": "poll",
        "tier": 2
    },
    "native": {
        "email": "",
        "password": "",
        "cookie": null,
        "interval": 300000
    },
    "instanceObjects": [
        {
            "_id": "system",
            "type": "channel",
            "common": {
                "name": "System"
            },
            "native": {}
        },
        {
            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Info"
            },
            "native": {}
        },
        {
            "_id": "info.userId",
            "type": "state",
            "common": {
                "role": "text",
                "name": "User Id",
                "type": "string",
                "read": true,
                "write": true,
                "def": ""
            },
            "native": {}
        },
        {
            "_id": "info.lastTourId",
            "type": "state",
            "common": {
                "role": "text",
                "name": "Last Tour Id",
                "type": "string",
                "read": true,
                "write": false,
                "def": ""
            },
            "native": {}
        },
        {
            "_id": "info.distance",
            "type": "state",
            "common": {
                "role": "text",
                "name": "Distance over all tours",
                "type": "string",
                "read": true,
                "write": false,
                "def": ""
            },
            "native": {}
        },
        {
            "_id": "info.movingTime",
            "type": "state",
            "common": {
                "role": "text",
                "name": "Time spent over all tours",
                "type": "string",
                "read": true,
                "write": false,
                "def": ""
            },
            "native": {}
        },
        {
            "_id": "info.followers",
            "type": "state",
            "common": {
                "role": "text",
                "name": "Followers",
                "type": "number",
                "read": true,
                "write": true,
                "def": 0
            },
            "native": {}
        },
        {
            "_id": "info.following",
            "type": "state",
            "common": {
                "role": "text",
                "name": "Following",
                "type": "number",
                "read": true,
                "write": true,
                "def": 0
            },
            "native": {}
        },
        {
            "_id": "system.cookie",
            "type": "state",
            "common": {
                "role": "text",
                "name": "Komoot Cookie",
                "type": "string",
                "read": true,
                "write": false,
                "def": ""
            },
            "native": {}
        }
    ]
}